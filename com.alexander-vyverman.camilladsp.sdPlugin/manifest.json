{
	"Name": "CamillaDSP",
	"Version": "0.1.0.0",
	"Author": "Alexander Vyverman",
	"Actions": [
		{
			"Name": "Counter",
			"UUID": "com.alexander-vyverman.camilladsp.increment",
			"Icon": "imgs/actions/counter/icon",
			"Tooltip": "Displays a count, which increments by one on press.",
			"Controllers": [
				"Keypad"
			],
			"States": [
				{
					"Image": "imgs/actions/counter/key",
					"TitleAlignment": "middle"
				}
			]
		},
		{
			"Name": "Volume",
				"UUID": "com.alexander-vyverman.camilladsp.volume",
				"Icon": "imgs/actions/mute/icon",
				"Tooltip": "changes the volume",
				"Controllers": [
					"Keypad", "Encoder"
				],
				"Encoder": {
					"layout": "$B1"},
				"States": [
					{
						"Image": "imgs/actions/mute/mute",
						"TitleAlignment": "middle"
					},
					{
						"Image": "imgs/actions/mute/unmute",
						"TitleAlignment": "middle"
					}]
		}
	],
	"Category": "CamillaDSP",
	"CategoryIcon": "imgs/plugin/category-icon",
	"CodePath": "bin/plugin.js",
	"Description": "Plugin to control the camillaDSP software",
	"Icon": "imgs/plugin/marketplace",
	"SDKVersion": 2,
	"Software": {
		"MinimumVersion": "6.5"
	},
	"OS": [
		{
			"Platform": "mac",
			"MinimumVersion": "10.15"
		},
		{
			"Platform": "windows",
			"MinimumVersion": "10"
		}
	],
	"Nodejs": {
		"Version": "20",
		"Debug": "enabled"
	},
	"UUID": "com.alexander-vyverman.camilladsp",
	"PropertyInspectorPath": "bin/property-inspector/inspect.html"
}
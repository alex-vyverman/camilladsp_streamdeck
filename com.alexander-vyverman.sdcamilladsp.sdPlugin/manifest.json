{
	"Name": "sdcamilladsp",
	"Version": "0.1.0.0",
	"Author": "Alexander Vyverman",
	"Actions": [
		{
			"Name": "Change configuration",
			"UUID": "com.alexander-vyverman.sdcamilladsp.change-config",
			"Icon": "imgs/actions/config/icons8-config",
			"Tooltip": "Set the Yaml file which will be sent to CamillaDSP upon clicking.",
			"PropertyInspectorPath": "ui/pi-change-config.html",
			"Controllers": [
				"Keypad"
			],
			"States": [
				{
					"Image": "",
					"TitleAlignment": "middle"
				}
			]
		},
		{
			"Name": "Main Volume Encoder",
			"UUID": "com.alexander-vyverman.sdcamilladsp.volume",
			"Icon": "imgs/actions/volume/icons8-voice",
			"Tooltip": "control the main Volume",
			"PropertyInspectorPath": "ui/pi-volume.html",
			"Encoder": {
				"layout": "$B1"
			},
			"Controllers": [
				"Encoder",
				"Keypad"
			],
			"States": [
				{
					"Image": "imgs/actions/volume/icons8-volume-up-white-72",
					"TitleAlignment": "middle"
				}
			]
		},
		{
			"Name": "Status information",
			"UUID": "com.alexander-vyverman.sdcamilladsp.status",
			"Icon": "imgs/actions/volume-button/icons8-volume-up-white-72",
			"Tooltip": "Shows the current status of the CamillaDSP server.",
			"PropertyInspectorPath": "ui/pi-volume-button.html",
			"Controllers": [
				"Keypad"
			],
			"States": [
				{
					"Image": "imgs/actions/volume-button/icons8-volume-up-white-72",
					"TitleAlignment": "middle"
				}
			]
		}
	],
	"Category": "sdcamilladsp",
	"CategoryIcon": "imgs/plugin/category-icon",
	"CodePath": "bin/plugin.js",
	"Description": "Control camillaDSP with StreamDeck",
	"Icon": "imgs/plugin/marketplace",
	"SDKVersion": 2,
	"Software": {
		"MinimumVersion": "6.4"
	},
	"OS": [
		{
			"Platform": "mac",
			"MinimumVersion": "12"
		},
		{
			"Platform": "windows",
			"MinimumVersion": "10"
		}
	],
	"Nodejs": {
		"Version": "20",
		"Debug": "enabled"
	},
	"UUID": "com.alexander-vyverman.sdcamilladsp"
